<?xml version="1.0"?>
<openerp>
    <data>
        <record model="ir.ui.view" id="email_check_wizard">
            <field name="name">email.check.wiz</field>
            <field name="model">mail.mail</field>
            <field name="type">tree</field>
            <field name="inherit_id" ref="mail.view_mail_tree"/>
            <field name="arch" type="xml">
                <field name="subject" string="mis"/>
                <field name="res_id" string="Email user"/>

            </field>
        </record>          
        
        <act_window id="launch_wizard_view_taxes"
                        name="View taxes"
                        src_model="sale.order"
                        res_model="mail.mail"
                        view_mode="tree"
                        domain="[(sale_order.name,'child_of','hola')]"
                        target="current"
                        key2="client_action_multi"/>          
     </data>
</openerp>
